{
  "name": "@arcxgame/contracts",
  "version": "5.2.0",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist/src/*",
    "dist/arc-types/*",
    "dist/deployments/*"
  ],
  "author": "Kerman Kohli <kermankohli@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn hardhat compile",
    "start": "node scripts/watcher.js",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts .",
    "clean": "rm -rf build/compiled && rm -rf build/flattened && rm -rf src/typings && rm -rf dist && rm -rf artifacts && rm -rf cache",
    "coverage": "yarn run build && yarn hardhat coverage --testfiles test/contracts/ --temp artifacts",
    "test": "yarn hardhat test",
    "test:ci": "npx mocha --reporter mocha-junit-reporter --require ts-node/register --require tsconfig-paths/register --require hardhat/register --exit",
    "release": "yarn release:local && release-it",
    "release:local": "rm -rf dist && ttsc && copyfiles -u 2 src/typings/*.d.ts dist/src/typings",
    "commit": "git add . && cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@typechain/ethers-v5": "^3.0.0",
    "@types/chai": "^4.2.8",
    "@types/fs-extra": "^9.0.3",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.0.0",
    "@types/node": "^13.7.0",
    "@types/prompt": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "axios": "^0.26.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.0",
    "commander": "^6.0.0",
    "commitizen": "^4.2.2",
    "copyfiles": "^2.4.1",
    "csv-writer": "^1.6.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-unused-imports": "^1.1.4",
    "ethereum-waffle": "^3.2.0",
    "fs-extra": "^9.0.1",
    "hardhat": "^2.8.3",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-preprocessor": "^0.1.1",
    "hardhat-spdx-license-identifier": "^2.0.1",
    "hardhat-typechain": "^0.3.3",
    "hardhat-watcher": "^2.0.0",
    "husky": "^4.3.0",
    "lodash": "^4.17.20",
    "mocha": "^9.1.3",
    "mocha-junit-reporter": "^2.0.2",
    "module-alias": "^2.2.2",
    "mustache": "^4.0.1",
    "neat-csv": "^6.0.1",
    "node-run-cmd": "^1.0.1",
    "prettier": "^2.0.5",
    "pretty-error": "^2.1.1",
    "prompt": "^1.2.2",
    "replace-in-file": "^6.1.0",
    "semantic-release": "^17.2.1",
    "shelljs": "^0.8.4",
    "solc": "^0.5.16",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.17",
    "system-commands": "^1.1.7",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.12",
    "typechain": "^4.0.0",
    "typescript": "^3.7.5"
  },
  "_moduleAliases": {
    "@src": "src",
    "@utils": "src/utils/",
    "@typings": "src/typings/",
    "@arc-types": "arc-types/",
    "@test": "test/",
    "@deployments": "deployments/"
  },
  "dependencies": {
    "@aave/core-v3": "^1.13.1",
    "@openzeppelin/contracts": "^4.3.2",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.1",
    "ethers": "5.0.2",
    "uuid": "^8.3.2"
  },
  "resolutions": {
    "ethereum-waffle/**/ethers": "5.0.2",
    "sc-istanbul": "^0.4.6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "description": "Contracts for the Synthetic Asset Game",
  "repository": "git@github.com:arcxio/contracts.git"
}
