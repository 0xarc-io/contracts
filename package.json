{
  "name": "@arcxgame/contracts",
  "version": "1.4.4",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "files": [
    "index.js",
    "dist",
    "publish/deployed/kovan/*.json",
    "publish/deployed/rinkeby/*.json",
    "publish/deployed/ropsten/*.json",
    "publish/deployed/mainnet/*.json"
  ],
  "author": "Kerman Kohli <kermankohli@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "push-patch": "yarn tsc && npm version patch && npm publish",
    "push-minor": "yarn tsc && npm version minor && npm publish",
    "compile": "yarn hardhat compile",
    "execute": "rm -rf dist && yarn ts-node publish/index.ts",
    "start": "node scripts/watcher.js",
    "lint:sol": "solium -d contracts/ --no-soliumignore",
    "clean": "rm -rf build/compiled && rm -rf build/flattened && rm -rf src/typings && rm -rf artifacts && rm -rf cache",
    "chain": "ganache-cli --networkId 50 --accounts 20 -l 19000000 -e 10000000000 -m 'concert load couple harbor equip island argue ramp clarify fence smart topic' -h 0.0.0.0",
    "generate-typings": "yarn ts-node ./bindings-generator/main.ts artifacts -o src/typings",
    "build": "yarn compile && yarn generate-typings",
    "coverage": "yarn run build && yarn hardhat coverage --testfiles test/contracts/ --temp artifacts --network coverage",
    "test": "yarn hardhat test",
    "deploy": "yarn ts-node deployment/deploy.ts",
    "graph:create": "graph create arcxgame/linkusd --node https://api.thegraph.com/deploy/",
    "graph:create-local": "graph create arcxmoney/linkusd --node http://127.0.0.1:8020",
    "graph:codegen": "graph codegen",
    "graph:build": "graph build",
    "graph:deploy": "graph deploy arcxgame/linkusd --access-token ff3f054dd82446549e745475ef50391d --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "graph:deploy-local": "graph deploy arcxmoney/linkusd --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "release": "rm -rf dist && yarn tsc && release-it"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@graphprotocol/graph-cli": "^0.18.0",
    "@graphprotocol/graph-ts": "^0.18.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@typechain/truffle-v4": "^2.0.2",
    "@typechain/truffle-v5": "^2.0.2",
    "@typechain/web3-v1": "^1.0.0",
    "@types/chai": "^4.2.8",
    "@types/fs-extra": "^9.0.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^13.7.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.0",
    "commander": "^6.0.0",
    "commitizen": "^4.2.2",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^2.5.1",
    "ethlint": "^1.2.4",
    "fs-extra": "^9.0.1",
    "hardhat": "^2.0.2",
    "hardhat-contract-sizer": "^2.0.1",
    "hardhat-preprocessor": "^0.1.1",
    "hardhat-spdx-license-identifier": "^2.0.1",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "node-run-cmd": "^1.0.1",
    "prettier": "^2.0.5",
    "pretty-error": "^2.1.1",
    "readline": "^1.3.0",
    "semantic-release": "^17.2.1",
    "shelljs": "^0.8.4",
    "solc": "^0.5.16",
    "solidifier": "^2.1.0",
    "system-commands": "^1.1.7",
    "table": "^5.4.6",
    "ts-generator": "0.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-jest": "^0.0.1",
    "typechain": "^2.0.0",
    "typescript": "^3.7.5",
    "web3-utils": "^1.2.11"
  },
  "_moduleAliases": {
    "@src": "src",
    "@test": "test",
    "@utils": "src/utils",
    "@typings": "src/typchain"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^2.5.0",
    "@openzeppelin/upgrades": "^2.8.0",
    "ethers": "^4.0.27",
    "husky": "^4.3.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "description": "Contracts for the Synthetic Asset Game",
  "repository": "git@github.com:arcxio/contracts.git"
}
