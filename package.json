{
  "name": "@arcxgame/contracts",
  "version": "5.2.0",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist/src/*",
    "dist/arc-types/*",
    "dist/deployments"
  ],
  "author": "Kerman Kohli <kermankohli@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "push-patch": "yarn ttsc && npm version patch && npm publish",
    "push-minor": "yarn ttsc && npm version minor && npm publish",
    "compile": "yarn hardhat compile",
    "execute": "rm -rf dist && yarn ts-node publish/index.ts",
    "start": "node scripts/watcher.js",
    "lint:sol": "solium -d contracts/ --no-soliumignore",
    "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts,.tsx .",
    "clean": "rm -rf build/compiled && rm -rf build/flattened && rm -rf src/typings && rm -rf dist && rm -rf artifacts && rm -rf cache",
    "chain": "ganache-cli --networkId 50 --accounts 20 -l 19000000 -e 10000000000 -m 'concert load couple harbor equip island argue ramp clarify fence smart topic' -h 0.0.0.0",
    "build": "yarn compile",
    "coverage": "yarn run build && yarn hardhat coverage --testfiles test/contracts/ --temp artifacts --network coverage",
    "test": "yarn hardhat test",
    "graph:create": "graph create arcxgame/contracts --node https://api.thegraph.com/deploy/",
    "graph:create-local": "graph create arcxgame/contracts --node http://127.0.0.1:8020",
    "graph:codegen": "graph codegen",
    "graph:build": "graph build",
    "graph:deploy-mainnet": "graph deploy arcxmoney/contracts --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "graph:deploy-playnet": "graph deploy gtupak/arcx-playnet --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "graph:deploy-kovan": "graph deploy arcxmoney/contracts-kovan --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "graph:deploy-rinkeby": "graph deploy arcxmoney/contracts-rinkeby --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "graph:deploy-local": "graph deploy arcxgame/contracts --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "graph:setup-local": "yarn graph:create-local && yarn graph:deploy-local",
    "release:local": "rm -rf dist && yarn ttsc",
    "release": "yarn release:local && release-it",
    "commit": "git add . && cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@graphprotocol/graph-ts": "^0.19.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@typechain/ethers-v5": "^3.0.0",
    "@types/chai": "^4.2.8",
    "@types/fs-extra": "^9.0.3",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.0.0",
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.0",
    "commander": "^6.0.0",
    "commitizen": "^4.2.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-unused-imports": "^1.1.4",
    "ethereum-waffle": "^3.2.0",
    "ethlint": "^1.2.4",
    "fs-extra": "^9.0.1",
    "hardhat": "^2.0.2",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-preprocessor": "^0.1.1",
    "hardhat-spdx-license-identifier": "^2.0.1",
    "hardhat-typechain": "^0.3.3",
    "hardhat-watcher": "^2.0.0",
    "husky": "^4.3.0",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "mustache": "^4.0.1",
    "neat-csv": "^6.0.1",
    "node-run-cmd": "^1.0.1",
    "prettier": "^2.0.5",
    "pretty-error": "^2.1.1",
    "replace-in-file": "^6.1.0",
    "semantic-release": "^17.2.1",
    "shelljs": "^0.8.4",
    "solc": "^0.5.16",
    "solidity-coverage": "^0.7.12",
    "system-commands": "^1.1.7",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.12",
    "typechain": "^4.0.0",
    "typescript": "^3.7.5"
  },
  "_moduleAliases": {
    "@src": "src",
    "@utils": "src/utils/",
    "@typings": "src/typings/",
    "@arc-types": "arc-types/",
    "@test": "test/"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^2.5.0",
    "ethers": "5.0.2"
  },
  "resolutions": {
    "ethereum-waffle/**/ethers": "5.0.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "description": "Contracts for the Synthetic Asset Game",
  "repository": "git@github.com:arcxio/contracts.git"
}
